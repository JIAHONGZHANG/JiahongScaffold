{
	"presets": [
		[
			"@babel/preset-env", {
				"targets": {
					"browsers": [
						"Chrome >= 59",
						"FireFox >= 44",
						"Safari >= 7",
						"Explorer 11",
						"last 4 Edge versions"
					  ]
				},
				"corejs": "2.6.5",
				"useBuiltIns": "usage"
			}
		],
		"@babel/preset-react"
	],
	"plugins": [
		["@babel/plugin-syntax-dynamic-import"],
		[
			"@babel/plugin-proposal-class-properties",
			{
			  "loose": true
			}
		],
		["import", {
			"libraryName": "antd",
			"libraryDirectory": "es",
			"style": "css"
		}]
	]
}